{
  "name": "syncit2.0",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nx build",
    "env-check": "dotenv-checker -s .env.example -e .env",
    "lint": "nx run-many --all --target=lint --parallel",
    "prepare": "husky && yarn env-check",
    "prepare:db": "yarn prisma:migrate && yarn prisma:generate && yarn prisma:push",
    "prisma:generate": "prisma generate --schema=./libs/models/src/prisma/prisma.schema",
    "prisma:migrate": "prisma migrate dev --schema=./libs/models/src/prisma/prisma.schema",
    "prisma:push": "prisma db push --schema=./libs/models/src/prisma/prisma.schema",
    "start": "nx run-many --all --target=serve --parallel --maxParallel=10",
    "test": "nx run-many --all --target=test  --codeCoverage --coverageReporters=json",
    "predev": "dotenv-checker --schema .env.schema --env .env.local"
  },
  "lint-staged": {
    "apps/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "libs/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.0.12",
    "@hookform/resolvers": "^3.9.0",
    "@nestjs/common": "^10.4.4",
    "@nestjs/core": "^10.4.4",
    "@nestjs/microservices": "^10.4.4",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/swagger": "^7.4.2",
    "@nrwl/next": "19.8.4",
    "@prisma/client": "^5.21.0",
    "amqp-connection-manager": "^4.1.7",
    "amqplib": "^0.10.3",
    "axios": "^1.1.2",
    "bcryptjs": "^2.4.3",
    "cookie": "^1.0.1",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.26.1",
    "daisyui": "^4.12.13",
    "googleapis": "^144.0.0",
    "ioredis": "^5.2.3",
    "next": "14.2.15",
    "pubsub-js": "^1.9.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.39.3",
    "recoil": "^0.7.5",
    "reflect-metadata": "^0.2.2",
    "regenerator-runtime": "0.14.1",
    "rxjs": "^7.0.0",
    "short-uuid": "^5.2.0",
    "tslib": "^2.3.0",
    "uuid": "^10.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.5.6",
