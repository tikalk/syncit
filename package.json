    "@nestjs/testing": "^10.4.4",
    "@nrwl/cli": "15.9.3",
    "@nrwl/cypress": "19.8.4",
    "@nrwl/eslint-plugin-nx": "19.8.4",
    "@nrwl/jest": "19.8.4",
    "@nrwl/linter": "19.8.4",
    "@nrwl/nest": "19.8.4",
    "@nrwl/node": "19.8.4",
    "@nrwl/react": "19.8.4",
    "@nrwl/web": "19.8.4",
    "@nrwl/workspace": "19.8.4",
    "@swc/core": "^1.3.38",
    "@swc/jest": "0.2.36",
    "@testing-library/react": "16.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.6.0",
    "@types/jest": "29.5.13",
    "@types/node": "22.7.5",
    "@types/react": "18.3.11",
